{
  "name": "jamming",
  "version": "1.0.0",
  "description": "Express server files for Quick Jammin'",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanesfast/jamming.git"
  },
  "author": "Shane Quick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shanesfast/jamming/issues"
  },
  "homepage": "https://github.com/shanesfast/jamming#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "find-config": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
